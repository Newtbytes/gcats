{
  "packages": {
    ".": {
      "package-name": "dev-scripts",
      "release-type": "simple",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false,
      "skip-github-release": true,
      "skip-changelog": true,
      "exclude-paths": [
        "beet.yml",
        "pakku.json",
        "pakku-lock.json",
        "datapack",
        "resourcepack",
        "README.md",
        "CHANGELOG.md",
        "**/*.png",
        ".pakku",
        "server.toml",
        "*.pw.toml",
        "pack"
      ]
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}